<%-- any content can be specified here e.g.: --%>
<%@ page pageEncoding="UTF-8" %>
<div id="logged" class="form">
    <form method="POST" action="logout.jsp" autocomplete="on"> 
        
        <p><label class="uname" ><%= session.getAttribute("username")%></label></p>
        <p><label class="email" ><%= session.getAttribute("email")%></label></p>
        
        <p class="signin button"> 
            <input type="submit" value="Log out"/> 
        </p>
    </form>
</div>